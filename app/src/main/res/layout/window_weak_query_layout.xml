<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/znnwin"
    android:orientation="vertical"
    android:paddingBottom="5dp">

    <LinearLayout
        android:id="@+id/inf_collTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="22dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:background="@drawable/znnwincontent"
            android:gravity="right"
            android:paddingRight="5dp"
            android:text="采集时间：" />

        <TextView
            android:id="@+id/ValueCollTime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/znnwincontent"
            android:gravity="center_horizontal"
            android:text="2018年12月21日 14时02分01秒"
            android:textSize="14dp" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="22dp"
        android:background="@drawable/znnwincontent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="22dp"
            android:background="@drawable/znnwincontent"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="right"
                android:paddingRight="5dp"
                android:text="CID/ECI：" />

            <TextView
                android:id="@+id/ValueECI"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center_horizontal"
                android:text="112332"
                android:textSize="14dp" />

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="right"
                android:paddingRight="5dp"
                android:text="LAC/TAC：" />

            <TextView
                android:id="@+id/ValueTAC"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center_horizontal"
                android:text="121212"
                android:textSize="14dp" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:layout_marginTop="22dp"
            android:background="@drawable/znnwincontent"
            android:orientation="horizontal">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="right"
                android:paddingRight="5dp"
                android:text="网络制式：" />

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center_horizontal"
                android:text="112332"
                android:textSize="14dp" />

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="right"
                android:paddingRight="5dp"
                android:text="信号强度：" />

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center_horizontal"
                android:text="121212"
                android:textSize="14dp" />
        </LinearLayout>


    </LinearLayout>






    <LinearLayout
        android:id="@+id/inf_GPS"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="22dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:background="@drawable/znnwincontent"
            android:gravity="right"
            android:paddingRight="5dp"
            android:text="GPS：" />

        <TextView
            android:id="@+id/ValueGPS"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/znnwincontent"
            android:gravity="center_horizontal"
            android:text="(12133.24343434,1212324.212121)"
            android:textSize="14dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/inf_phoneNumber"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="22dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:background="@drawable/znnwincontent"
            android:gravity="right"
            android:paddingRight="5dp"
            android:text="手机号码：" />

        <TextView
            android:id="@+id/ValuePhoneNumber"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/znnwincontent"
            android:gravity="center_horizontal"
            android:text="18785185684"
            android:textSize="14dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/inf_overlayScene"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="22dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:background="@drawable/znnwincontent"
            android:gravity="right"
            android:paddingRight="5dp"
            android:text="覆盖场景：" />

        <TextView
            android:id="@+id/ValueOverlayScene"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/znnwincontent"
            android:gravity="center_horizontal"
            android:text="城区_党政军"
            android:textSize="14dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/inf_district"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="22dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:background="@drawable/znnwincontent"
            android:gravity="right"
            android:paddingRight="5dp"
            android:text="区县：" />

        <TextView
            android:id="@+id/ValueDistrict"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/znnwincontent"
            android:gravity="center_horizontal"
            android:text="凯里市"
            android:textSize="14dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/inf_address"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="22dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:background="@drawable/znnwincontent"
            android:gravity="right"
            android:paddingRight="5dp"
            android:text="地址：" />

        <TextView
            android:id="@+id/ValueAddress"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/znnwincontent"
            android:gravity="center_horizontal"
            android:text="某某区某某街道"
            android:textSize="14dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/inf_NetworkOperatorName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="22dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:background="@drawable/znnwincontent"
            android:gravity="right"
            android:paddingRight="5dp"
            android:text="运营商：" />

        <TextView
            android:id="@+id/ValueNetworkOperatorName"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/znnwincontent"
            android:gravity="center_horizontal"
            android:text="移动4G"
            android:textSize="14dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/inf_solveStatus"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="22dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:background="@drawable/znnwincontent"
            android:gravity="right"
            android:paddingRight="5dp"
            android:text="状态：" />

        <TextView
            android:id="@+id/ValueSolveStatus"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/znnwincontent"
            android:gravity="center_horizontal"
            android:text="未处理"
            android:textSize="14dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/inf_solveTime"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="22dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:background="@drawable/znnwincontent"
            android:gravity="right"
            android:paddingRight="5dp"
            android:text="变更时间：" />

        <TextView
            android:id="@+id/ValueSolveTime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/znnwincontent"
            android:gravity="center_horizontal"
            android:text="2018年12月21日 14时02分01秒"
            android:textSize="14dp" />
    </LinearLayout>

</LinearLayout>